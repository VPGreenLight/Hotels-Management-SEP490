{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SQLServer": "{CONNECTIONSTRINGS_DEFAULTCONNECTION}"
  },
  "EmailConfiguration": {
    "From": "{EMAILCONFIGURATION_FROM}",
    "SmtpServer": "{EMAILCONFIGURATION_SMTP_SERVER}",
    "Port": "{EMAILCONFIGURATION_PORT}",
    "Username": "{EMAILCONFIGURATION_USERNAME}",
    "Password": "{EMAILCONFIGURATION_PASSWORD}",
    "TrackingEmail": "{EMAILCONFIGURATION_TRACKINGEMAIL}",
    "TrackingPassword": "{EMAILCONFIGURATION_PASSWORD}"
  },
  "CloudflareR2": {
    "AccessKey": "{CLOUDFLARER2_ACCESS_KEY}",
    "SecretKey": "{CLOUDFLARER2_SECRET_KEY}",
    "Bucket": "{CLOUDFLARER2_BUCKET}",
    "AccountId": "{CLOUDFLARER2_ACCOUNT_ID}",
    "PublicUrl": "{CLOUDFLARER2_PUBLIC_URL}",
    "Region": "{CLOUDFLARER2_REGION}"
  },
  "JWT": {
    "Authority": "{JWT_AUTHORITY}",
    "ValidAudience": "{JWT_VALID_AUDIENCE}",
    "ValidIssuer": "{JWT_VALID_ISSUER}",
    "Secret": "JWT_SECRET",
    "TokenValidityInHours": "{JWT_TOKEN_VALIDITY_IN_HOURS}",
    "RefreshTokenValidityInDays": "{JWT_TOKEN_VALIDITY_IN_DAYS}"
  }
}
